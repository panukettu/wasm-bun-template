{
  "entries": ["./wasm/index.ts"],
  "targets": {
    "debug": {
      "outFile": "web/static/debug.wasm",
      "textFile": "web/static/debug.wat",
      "sourceMap": true,
      "debug": true
    },
    "release": {
      "outFile": "web/static/release.wasm",
      "textFile": "web/static/release.wat",
      "sourceMap": true,
      "optimizeLevel": 3,
      "shrinkLevel": 0,
      "converge": false,
      "noAssert": false
    }
  },
  "options": {
    "bindings": "esm"
  }
}
